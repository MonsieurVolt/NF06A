#include <stdio.h>
#include <math.h>

int main() {
    int NTimes;
    printf("How many different squares do you want ? : ");
    scanf("%d",&NTimes);
    for (int i = 0; i < NTimes; i++) {
        printf("What number do you want ? : ");
        float nb;
        scanf("%f",&nb);
        if (nb < 0) {
            printf("You can't have a square root on %f",nb);
        
        } else {
            
            printf("the square root is %.3f",sqrt(nb));
        }
    }
}   